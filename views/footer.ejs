<!-- Toastr options -->
<script>
    
        toastr.options = {
            "closeButton": true,
            "debug": false,
            "newestOnTop": true,
            "progressBar": true,
            "positionClass": "toast-bottom-right",
            "preventDuplicates": false,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "3000",
            "extendedTimeOut": "1500",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
    
</script>

<!-- Display toast after actions -->
<script>
    if (sessionStorage.getItem("reg")=='1'){
        toastr.success("User registered successfully!", "Notification:");
        sessionStorage.removeItem("reg");
    } else if (sessionStorage.getItem("del")=='1'){
        toastr.success("User deleted successfully", "Notification:");
        sessionStorage.removeItem("del");
    } else if (sessionStorage.getItem("edit")=='1'){
        toastr.success("User edited successfully", "Notification:");
        sessionStorage.removeItem("edit");
    } else if (sessionStorage.getItem("ser")=='1'){
        toastr.success("Service deleted successfully", "Notification:");
        sessionStorage.removeItem("ser");
    } else if (sessionStorage.getItem("sered")=='1'){
        toastr.success("Service edited successfully", "Notification:");
        sessionStorage.removeItem("sered");
    } else if (sessionStorage.getItem("serad") =='1') {
        toastr.success("Service added successfully", "Notification:");
        sessionStorage.removeItem("serad");
    }else if (sessionStorage.getItem("delb")=='1'){
        toastr.success("Booking deleted successfully", "Notification:");
        sessionStorage.removeItem("delb");
    }
</script>

